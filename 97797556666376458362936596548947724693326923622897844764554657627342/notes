var state=false
function stylechange()
{
	var x = document.getElementById("style");
	if (state===true) {
		x.className=("spacer loggocol")
		state=false
	} else if(state===false){
		x.className=("spacer white")
		state=true
	}
	//	x.classList.exsists()
}





